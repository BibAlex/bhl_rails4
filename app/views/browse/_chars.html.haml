.clearfix
.alphabet  
  %ul.list-inline
    %li
      - if @char == ''
        %b
          = I18n.t('common.all')
      - else
        = link_to(I18n.t('common.all'), { controller: "browse", action: "index", type: params[:type] })
    - @alphas.each do |alpha|
      %li 
        - if @char.downcase == alpha.downcase
          %b
            = alpha
        - else
          = link_to(alpha, { controller: "browse", action: "index", type: params[:type], char: "#{alpha.downcase}" })